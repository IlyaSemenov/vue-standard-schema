{
  "name": "vue-valibot",
  "type": "module",
  "version": "3.1.2",
  "packageManager": "pnpm@9.8.0",
  "description": "Vue3 composables for handling form submit with optional valibot integration",
  "author": "Ilya Semenov",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/IlyaSemenov/vue-valibot"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "main": "dist/index.cjs",
  "types": "dist/index.d.ts",
  "files": [
    "!src/**/*.test.*",
    "dist",
    "src"
  ],
  "scripts": {
    "build": "tsup",
    "lint": "eslint --fix '**/*.{cjs,js,ts}'",
    "prepare": "husky",
    "prepublishOnly": "pnpm build",
    "test": "vitest run && tsd"
  },
  "peerDependencies": {
    "@vue/reactivity": "^3",
    "@vue/runtime-core": "^3",
    "valibot": ">=0.32 <2"
  },
  "devDependencies": {
    "@changesets/cli": "^2.26.2",
    "@ilyasemenov/eslint-config": "^1.4.2",
    "@vue/reactivity": "^3.4.29",
    "@vue/runtime-core": "^3.4.29",
    "eslint": "^9.35.0",
    "husky": "^9.1.7",
    "tsconfig-vite-node": "^1.1.2",
    "tsd": "^0.31.0",
    "tsup": "^8.0.2",
    "typescript": "^5.4.5",
    "valibot": "^1.0.0-beta.3",
    "vite-tsconfig-paths": "^4.3.2",
    "vitest": "^1.5.3"
  }
}
