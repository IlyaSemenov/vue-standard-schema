{
  "name": "vue-standard-schema",
  "type": "module",
  "version": "0.0.0",
  "packageManager": "pnpm@9.8.0",
  "description": "Vue3 composables for handling form submit with Standard Schema validation support",
  "author": "Ilya Semenov",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/IlyaSemenov/vue-standard-schema"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "main": "dist/index.cjs",
  "types": "dist/index.d.ts",
  "files": [
    "!src/**/*.test.*",
    "dist",
    "src"
  ],
  "scripts": {
    "build": "tsup",
    "lint": "eslint --fix '**/*.{cjs,js,ts}'",
    "prepare": "husky",
    "prepublishOnly": "pnpm build",
    "test": "vitest run --typecheck"
  },
  "peerDependencies": {
    "@vue/reactivity": "^3",
    "@vue/runtime-core": "^3"
  },
  "dependencies": {
    "@standard-schema/spec": "^1.0.0"
  },
  "devDependencies": {
    "@changesets/cli": "^2.26.2",
    "@ilyasemenov/eslint-config": "^1.4.2",
    "@vue/reactivity": "^3.4.29",
    "@vue/runtime-core": "^3.4.29",
    "eslint": "^9.35.0",
    "husky": "^9.1.7",
    "tsconfig-vite-node": "^1.2.0",
    "tsup": "^8.5.0",
    "typescript": "^5.4.5",
    "valibot": "^1.1.0",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.4",
    "zod": "^4.1.7"
  }
}
