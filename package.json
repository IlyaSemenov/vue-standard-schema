{
  "name": "vue-valibot-form",
  "type": "module",
  "version": "2.0.0",
  "description": "Minimalistic Vue3 composable for handling form submit",
  "author": "Ilya Semenov",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/IlyaSemenov/vue-valibot-form"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "main": "dist/index.cjs",
  "types": "dist/index.d.ts",
  "files": [
    "!src/**/*.test.*",
    "dist",
    "src"
  ],
  "scripts": {
    "build": "tsup",
    "lint": "eslint --fix '**/*.{cjs,js,ts}'",
    "prepare": "husky",
    "prepublishOnly": "pnpm build",
    "test": "vitest run && tsd"
  },
  "peerDependencies": {
    "@vue/reactivity": "^3",
    "valibot": ">=0.19 <1"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.16.1",
    "@changesets/cli": "^2.26.2",
    "@vue/reactivity": "^3.3.4",
    "husky": "^9.0.11",
    "tsconfig-vite-node": "^1.1.2",
    "tsd": "^0.31.0",
    "tsup": "^8.0.2",
    "typescript": "^5.4.5",
    "valibot": "^0.19.0",
    "vite-tsconfig-paths": "^4.3.2",
    "vitest": "^1.5.3"
  }
}
